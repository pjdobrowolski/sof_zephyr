
PHDRS {
    _rodata_phdr PT_LOAD;
}

EXTERN(HPSRAM)

SECTIONS {
    ..rodata : ALIGN(4096) {
        __rodata_start = ABSOLUTE(.);
        *(.gnu.linkonce.r.*)
        *(.rodata)
        *(.rodata.*)
        KEEP (*(.eh_frame))
        __rodata_end = ABSOLUTE(.);
    } >HPSRAM_seg AT>IMR_seg : _rodata_phdr

    /*!!! REMOVE THIS ??? */
    .module : ALIGN(4096) {
        KEEP(*(.module))
    } >HPSRAM_seg AT>IMR_seg : _rodata_phdr
}
